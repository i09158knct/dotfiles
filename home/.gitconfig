[include]
  path = .gitconfig.local
[color]
  ui = auto
[tig "bind"]
  generic = g move-first-line
  generic = G move-last-line
[core]
  excludesfile = ~/.gitignore
  editor = /usr/bin/vim
[alias]
  b = branch
  ci = commit
  co = checkout
  d = diff
  l = log
  st = status
  sub = submodule
